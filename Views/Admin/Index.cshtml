@model IEnumerable<OCD_task.ViewModel.EmployeeVM>



<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Address</th>
      <th scope="col">Job</th>
      <th scope="col">Age</th>

    </tr>
  </thead>
  <tbody>
        @foreach (var item in @Model)
        {

        <tr>
          <th scope="row">@item.Id</th>
          <td>@item.Name</td>
          <td>@item.Email</td>
          <td>@item.phone</td>
          <td>@item.Address</td>
          <td>@item.Job</td>
          <td>@item.Age</td>

        </tr>
        }

  </tbody>
</table>